{
    "name": "elephantsgroup/eg-news",
    "description": "Yii2 news management module",
    "keywords": ["yii", "yii2", "news", "elephantsgroup"],
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Jalal Jaberi",
            "email": "j_jaberi@yahoo.com"
        },
        {
            "name": "Arezou Zahedi Majd",
            "email": "a_majd_8@yahoo.com"
        },
        {
            "name": "Arvin Firouzi",
            "email": "arvin.firouzi@yahoo.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "elephantsgroup/eg-base": "~1",
        "elephantsgroup/eg-stat": "~1",
        "elephantsgroup/eg-like": "~1",
        "elephantsgroup/eg-follow": "~1",
        "elephantsgroup/eg-star-rating": "~1",
        "elephantsgroup/eg-comment": "~1",
        "elephantsgroup/eg-jdf": "~1",
		"mihaildev/yii2-ckeditor": "*",
		"elephantsgroup/eg-jdate": "~1",
		"kartik-v/yii2-widget-timepicker": "*"
    },
    "config": {
        "process-timeout": 1500,
        "secure-http": false
    },
    "autoload": {
        "psr-4": { "elephantsGroup\\news\\": "" }
    }
}